{
  "name": "spookfox",
  "version": "0.1.13",
  "license": "GPLv3",
  "scripts": {
    "start": "parcel serve src/manifest.json 'src/apps/*.ts'",
    "build": "parcel build src/manifest.json 'src/apps/*.ts'",
    "lint": "eslint src/ && tsc --noEmit",
    "publish-addon": "yarn build && web-ext sign --channel=unlisted -s ./dist -a ./dist/addons --api-key $FIREFOX_ADDON_KEY --api-secret $FIREFOX_ADDON_SECRET"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "parcel": "^2.3.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "web-ext": "^6.7.0",
    "web-ext-types": "^3.2.1"
  },
  "dependencies": {
    "immer": "^9.0.12",
    "uuid": "^8.3.2"
  }
}
